    "postcss": "^8.2.10",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-block-reveal-animation": "^1.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "husky": "^5.0.8",
    "prettier": "2.2.1"
