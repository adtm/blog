{
  "name": "personal-website",
  "private": false,
  "description": "Personal website",
  "version": "1.0.0",
  "author": "Tomas Eglinskas <tomas.eglinskas@gmail.com>",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "develop": "gatsby develop",
    "serve": "gatsby build && gatsby serve",
    "start": "npm run build:style && gatsby develop",
    "lint": "eslint src/**/*.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.tsx\"",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@tailwindcss/postcss7-compat": "^2.1.2",
    "autoprefixer": "^9.8.6",
    "gatsby": "^2.30.1",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-feed-mdx": "^1.0.1",
    "gatsby-plugin-google-analytics": "^2.9.0",
    "gatsby-plugin-layout": "^1.8.0",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-mdx": "^1.8.0",
    "gatsby-plugin-offline": "^3.8.0",
    "gatsby-plugin-page-creator": "^2.8.0",
    "gatsby-plugin-page-transitions": "^1.0.8",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sharp": "^2.12.0",
